{
  "name": "expired-sveltekit",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev --host",
    "dev:https": "svelte-kit dev --https --host",
    "dev:functions": "npx wrangler pages dev /functions",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.3.1",
    "@iconify/svelte": "^2.1.2",
    "@sentry/browser": "^6.17.6",
    "@sentry/tracing": "^6.17.6",
    "@supabase/supabase-js": "^1.30.0",
    "@sveltejs/adapter-cloudflare": "next",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "cypress": "^9.4.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-svelte3": "^3.2.1",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.9.4",
    "svelte-windicss-preprocess": "^4.2.5",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "type": "module",
  "dependencies": {
    "cookie": "^0.4.2",
    "cookie-parser": "^1.4.6",
    "date-fns": "^2.28.0"
  }
}
